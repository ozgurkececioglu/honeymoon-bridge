{
  "name": "@honeymoon-bridge/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev:be": "npm run start --workspace=@honeymoon-bridge/backend",
    "start:dev:fe": "npm run start --workspace=@honeymoon-bridge/frontend",
    "build:be": "npm run build --workspace=@honeymoon-bridge/backend",
    "build:fe": "npm run build --workspace=@honeymoon-bridge/frontend",
    "build": "npm run build:be && npm run build:fe",
    "test:be": "npm run test --workspace=@honeymoon-bridge/backend",
    "test:fe": "npm run test --workspace=@honeymoon-bridge/frontend",
    "test": "npm run test:be && npm run test:fe",
    "lint:be": "npm run lint --workspace=@honeymoon-bridge/backend",
    "lint:fe": "npm run lint --workspace=@honeymoon-bridge/frontend",
    "lint": "npm run lint:be && npm run lint:fe",
    "generate:swagger": "./bin/generate-swagger-api.sh"
  },
  "private": true,
  "devDependencies": {
    "prettier": "^2.6.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
